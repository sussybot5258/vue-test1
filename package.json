{
  "name": "test1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "preview": "bunx --bun vite preview",
    "check": "bunx --bun vue-tsc --noEmit",
    "build": "bun check && bun build-only",
    "build-only": "bunx --bun vite build --minify false",
    "build:submodule": "bun check && bun build-only:submodule",
    "build-only:submodule": "bunx --bun vite build --minify false --mode submodule"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@types/bun": "^1.0.7",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vue-tsc": "^1.8.27"
  }
}
